{
  "name": "rsm-application",
  "version": "0.0.1",
  "description": "rsm",
  "main": "src/app.js",
  "scripts": {
    "electron": "set ELECTRON_ENABLE_LOGGING=true && electron src/client.js --debug &",
    "dev": "webpack-dev-server  --config webpack.config.dev.js --devtool eval --progress --colors --watch --inline ",
    "dll": "webpack --config webpack.config.dll.js",
    "unit": "node_modules/.bin/jest",
    "coverage": "node_modules/.bin/jest --coverage",
    "build": "webpack --config webpack.config.js --devtool source-map",
    "start": "SET BABEL_ENV=production && webpack --config webpack.config.js --profile",
    "build-proxy": "webpack --config webpack.config.proxy.js ",
    "releases": "npm run start && node_modules/.bin/electron-packager client rsm-application  --electronVersion=1.7.5   --out=releases  --overwrite ",
    "package": "node_modules/.bin/electron-packager client rsm-application  --electronVersion=1.7.5 --platform=win32   --out=releases  --overwrite --asar=true"
  },
  "config": {
    "serverAddress": ""
  },
  "build": {
    "app-bundle-id": "your.id",
    "app-category-type": "your.app.category.type",
    "iconUrl": "(windows only) A URL to an ICO file to use as the application icon, see details below"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/jaxchow/rms-application.git"
  },
