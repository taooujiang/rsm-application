    "babel-preset-es2015": "^6.0.12",
    "babel-preset-react": "^6.0.15",
    "babel-preset-stage-0": "^6.3.13",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.5.2",
    "copy-webpack-plugin": "^4.0.1",
